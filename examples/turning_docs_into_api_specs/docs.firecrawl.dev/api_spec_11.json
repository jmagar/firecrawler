                      "ogImage": "https://docs.firecrawl.dev/mendable_new_og1.png",
                      "ogLocaleAlternate": [],
                      "ogSiteName": "Mendable",
                      "ogTitle": "Mendable",
                      "ogUrl": "https://docs.firecrawl.dev/",
                      "robots": "follow, index",
                      "sourceURL": "https://docs.firecrawl.dev/",
                      "title": "Mendable"
                    }
                  },
                  "success": true
                },
                "schema": {
                  "properties": {
                    "data": {
                      "properties": {
                        "content": {
                          "type": "string"
                        },
                        "llm_extraction": {
                          "type": "object"
                        },
                        "metadata": {
                          "type": "object"
                        }
                      },
                      "type": "object"
                    },
                    "success": {
                      "type": "boolean"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "Successful scrape"
