{
  "info": {
    "title": "Firecrawl API",
    "version": "1.0.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "/check_crawl_status": {
      "post": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "current": {
                      "type": "integer"
                    },
                    "data": {
                      "items": {
                        "properties": {
                          "content": {
                            "type": "string"
                          },
                          "markdown": {
                            "type": "string"
                          },
                          "metadata": {
                            "properties": {
                              "description": {
                                "type": "string"
                              },
                              "language": {
                                "type": "string"
                              },
                              "sourceURL": {
                                "type": "string"
                              },
                              "title": {
                                "type": "string"
                              }
                            },
                            "type": "object"
                          },
                          "provider": {
                            "type": "string"
                          }
                        },
                        "type": "object"
                      },
                      "type": "array"
                    },
                    "status": {
                      "type": "string"
                    },
                    "total": {
                      "type": "integer"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "Crawl job status"
          }
        },
        "summary": "Check crawl job status"
      }
    },
    "/crawl": {
      "post": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "jobId": {
                      "type": "string"
                    }
                  },
                  "type": "object"
                }
              }
            },
            "description": "Job ID"
          }
        },
        "summary": "Crawl URL and subpages"
      }
    }
  }
}